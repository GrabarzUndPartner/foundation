{
  "name": "foundation",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "postinstall": "husky install",
    "test": "npm run lint && npm run build",
    "dev": "npm run dev --workspace=@foundation/sample",
    "build": "npm run build --workspace=@foundation/sample",
    "start": "npm run start --workspace=@foundation/sample",
    "lint": "npm run lint:es && npm run lint:css",
    "lint:es": "eslint --ext .js,.vue .",
    "lint:css": "stylelint \"packages/**/*.vue\""
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.16.3",
    "@commitlint/cli": "15.0.0",
    "@commitlint/config-conventional": "15.0.0",
    "@js-basics/vector": "^1.57.3",
    "@mapbox/stylelint-processor-arbitrary-tags": "0.4.0",
    "@nuxtjs/eslint-config": "^7.0.0",
    "babel-preset-vue": "2.0.2",
    "branch-name-lint": "2.1.1",
    "commitlint": "15.0.0",
    "eslint": "8.3.0",
    "eslint-formatter-json-relative": "0.1.0",
    "eslint-plugin-html": "6.2.0",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-no-secrets": "0.8.9",
    "eslint-plugin-no-unsanitized": "4.0.0",
    "eslint-plugin-nuxt": "3.0.0",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-vue": "8.1.1",
    "eslint-plugin-xss": "0.1.11",
    "husky": "7.0.4",
    "lint-staged": "12.1.2",
    "postcss-html": "^1.3.0",
    "rxjs": "^7.4.0",
    "standard": "16.0.4",
    "stylelint": "14.1.0",
    "stylelint-config-css-modules": "2.3.0",
    "stylelint-config-lost": "0.0.3",
    "stylelint-config-recess-order": "3.0.0",
    "stylelint-config-standard": "24.0.0",
    "vue-slick-carousel": "^1.0.6"
  },
  "workspaces": [
    "./packages/core",
    "./packages/sample"
  ]
}
